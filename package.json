{
  "name": "stencil-starter-project-name",
  "private": true,
  "version": "0.0.1",
  "description": "Stencil App Starter",
  "scripts": {
    "build": "stencil build",
    "start:old": "stencil build --dev --watch --serve",
    "start": "concurrently \"stencil build --dev --watch --serve\" \"node scripts/watchsass\"",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "dependencies": {
    "@stencil/core": "^1.8.6",
    "@stencil/router": "^1.0.1",
    "@stencil/sass": "^1.1.1",
    "stencil-fragment": "^1.0.1"
  },
  "license": "MIT",
  "devDependencies": {
    "chokidar": "^3.3.1",
    "concurrently": "^4.1.2"
  }
}
